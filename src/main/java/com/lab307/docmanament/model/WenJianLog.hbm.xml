<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lab307.docmanament.model.WenJianLogModel" table="t_wjlog">
        <id name="bh" column="bh" type="java.lang.String" length="32">
            <generator class="assigned"></generator>
        </id>

        <property
            name="qybh"
            column="qybh"
            type="java.lang.String"
            length="32"
        />
                
        <property
            name="wjbh"
            column="wjbh"
            type="java.lang.String"
            length="32"
        />
                
        <property
            name="cz"
            column="cz"
            type="java.lang.String"
            length="20"
        />

        <property
            name="czr"
            column="czr"
            type="java.lang.String"
            length="32"
        />
        
        <property
            name="czsj"
            column="czsj"
            type="java.util.Calendar"
        />

        <property
            name="bz"
            column="bz"
            type="java.lang.String"
            length="50"
        />
    </class>
</hibernate-mapping>